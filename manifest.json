{
  "name": "Custom emoticons",
  "version": "2",
  "manifest_version": 2,
  "description": "custom emoticons on facebook",
  "icons": { 
    "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png" 
    },
    "content_scripts": [
    {
      "matches": ["https://www.facebook.com/*"],
      "run_at" : "document_end",
      "all_frames": true,
      "js": ["script.js"]
    }
  ]
}